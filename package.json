{
    "author": "Steven Olmsted <steven.olmsted@ibigroup.com>",
    "babel": {
        "env": {
            "build": {
                "presets": [
                    "minify"
                ]
            }
        },
        "presets": [
            "env"
        ],
        "sourceMaps": "inline"
    },
    "bugs": "https://github.com/ibi-group/babel-plugin-transform-line/issues",
    "contributors": [
        "Duane Sibilly <duane.sibilly@ibigroup.com>"
    ],
    "dependencies": {
        "babel-runtime": "~6.26.0"
    },
    "description": "Replaces the identifier __line with the source line number.",
    "devDependencies": {
        "babel-cli": "~6.26.0",
        "babel-istanbul": "~0.12.2",
        "babel-preset-env": "~1.6.0",
        "babel-preset-minify": "~0.2.0",
        "chai": "~4.1.2",
        "eslint": "~4.6.1",
        "eslint-plugin-isotropic": "~0.1.0",
        "mocha": "~3.5.1",
        "nsp": "~2.8.0"
    },
    "eslintConfig": {
        "env": {
            "browser": true,
            "es6": true,
            "node": true
        },
        "extends": "plugin:isotropic/isotropic",
        "parserOptions": {
            "ecmaFeatures": {
                "experimentalObjectRestSpread": true,
                "globalReturn": false,
                "impliedStrict": true,
                "jsx": false
            },
            "ecmaVersion": 2017,
            "sourceType": "module"
        },
        "plugins": [
            "isotropic"
        ],
        "root": true
    },
    "homepage": "https://github.com/ibi-group/babel-plugin-transform-line",
    "license": "BSD-3-Clause",
    "main": "lib/babel-plugin-transform-line.js",
    "name": "babel-plugin-transform-line",
    "repository": "ibi-group/babel-plugin-transform-line",
    "scripts": {
        "build": "BABEL_ENV=build babel js -d lib",
        "lint": "eslint js test",
        "posttest": "[ -z \"$npm_config_coverage\" ] || babel-istanbul check-coverage --branches 100 --functions 100 --lines 100 --statements 100",
        "prepare": "npm run build",
        "prepublishOnly": "npm run securityCheck && npm test --coverage",
        "pretest": "npm run lint",
        "securityCheck": "nsp check",
        "test": "BABEL_DISABLE_CACHE=1 babel-node --plugins ./babel-plugin-transform-some-other-dynamic-code.js,../js/babel-plugin-transform-line.js ./node_modules/.bin/babel-istanbul test _mocha"
    },
    "version": "0.4.0"
}
